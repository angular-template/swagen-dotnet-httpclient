{
    "output": "./output.cs",
    "generator": "dotnet-httpclient",
    "mode": "csharp",
    "transforms": {
        "serviceName": "pascalCase",
        "operationName": "pascalCase",
        "modelName": "pascalCase",
        "propertyName": "pascalCase",
        "parameterName": "camelCase"
    },
    "prefixLines": [
        "// ReSharper disable All"
    ],
    "url": "http://petstore.swagger.io/v2/swagger.json",
    "options": {
        "generate": {
            "implementation": true,
            "models": true,
            "interfaces": true,
            "shared": true
        },
        "namespaces": {
            "services": "MyNamespace.Services",
            "models": "MyNamespace.Services"
        },
        "accessLevel": "public",
        "skipModelsNsPrefix": true
    }
}
