{
    "name": "swagen-dotnet-httpclient",
    "version": "1.0.0-beta.22",
    "description": "Swagen generators for .NET HttpClient",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts": {
        "lint": "eslint ./**/*.js",
        "bump": "npm version prerelease --no-git-tag-version",
        "prepublishOnly": "npm run lint",
        "postpublish": "npm run bump",
        "update": "npm un --save codewriter lodash node-html-encoder swagen-csharp-language && npm i --save codewriter lodash node-html-encoder swagen-csharp-language",
        "test": "node ./test-harness"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swagen/swagen-dotnet-httpclient.git"
    },
    "keywords": [
        "swagen",
        "dotnet",
        "csharp",
        "httpclient"
    ],
    "author": "Swagen <swagen@jeevanjames.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/swagen/swagen-dotnet-httpclient/issues"
    },
    "homepage": "https://github.com/swagen/swagen-dotnet-httpclient#readme",
    "dependencies": {
        "codewriter": "^1.3.0",
        "lodash": "^4.17.11",
        "node-html-encoder": "0.0.2",
        "swagen-csharp-language": "^1.0.0-beta.4"
    },
    "devDependencies": {
        "eslint": "^5.13.0"
    }
}
